@using MarkUps.Areas.Admin.ViewModels.Setting;
@using MarkUps.Models;
@model SettingUpdateVM

<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Update Setting</h4>
            <form class="forms-sample" method="post">
                <div asp-validation-summary="ModelOnly"></div>
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input asp-for="Key" class="form-control" placeholder="Key">
                    <span class="text-danger" asp-validation-for="Key"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Value">Value</label>
                    <input asp-for="Value" class="form-control" placeholder="Value">
                    <span class="text-danger" asp-validation-for="Value"></span>
                </div>
                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <a class="btn btn-light" asp-action="index" asp-controller="setting">Cancel</a>
            </form>
        </div>
    </div>
</div>